{
  "name": "scaffold-static",
  "version": "1.0.2",
  "description": "Start out building static websites powered by vanilla-js with this CLI tool",
  "main": "index.js",
  "files": [
    "src"
  ],
  "bin": {
    "scaffold-static": "./bin/index.js"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test/main_test.js",
    "lint": "node ./node_modules/eslint/bin/eslint ./bin ./src",
    "lint:fix": "node ./node_modules/eslint/bin/eslint --fix ./bin ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": "npm run lint:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesgeorge007/scaffold-static.git"
  },
  "keywords": [
    "nodejs",
    "cli",
    "inquirer.js",
    "commander.js"
  ],
  "author": "James George<jamesgeorge998001@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jamesgeorge007/scaffold-static/issues"
  },
  "homepage": "https://github.com/jamesgeorge007/scaffold-static#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "commander": "^2.19.0",
    "figlet": "^1.2.1",
    "inquirer": "^6.2.1",
    "mkdirp": "^0.5.1",
    "open": "^6.3.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "mocha": "^5.2.0",
    "prettier": "^1.17.1"
  }
}
